{
  "schemaVersion": 1,
  "id": "cidb",
  "version": "${version}",
  "name": "CIDB",
  "description": "Change colors of item durability bar to what you like!",
  "authors": [
    "XeronioDiR"
  ],
  "contact": {},
  "license": "CC-",
  "icon": "assets/cidb/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "net.xeroniodir.cidb.client.CidbDataGenerator"
    ],
    "client": [
      "net.xeroniodir.cidb.client.CidbClient"
    ],
    "main": [
      "net.xeroniodir.cidb.Cidb"
      ],
      "modmenu": [
        "net.xeroniodir.cidb.client.ModMenuIntegrator"
      ]
  },
  "mixins": [
    "cidb.mixins.json",
    {
      "config": "cidb.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${min_minecraft_version} <=${max_minecraft_version}",
    "yet_another_config_lib_v3": ">=${yacl_version}"
  }
}
